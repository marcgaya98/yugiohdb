{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript"
  ],
  "files.exclude": {
    "**/node_modules": true
  },
  "files.associations": {
    ".github/CODEOWNERS": "plaintext"
  },
  "dockerfile-utils.excludePaths": [
    ".github/CODEOWNERS"
  ]
}